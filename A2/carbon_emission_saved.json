{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 200,
  "data": {
    "url": "https://raw.githubusercontent.com/aaronmonash24/FIT3179/main/emission_saved.csv"
  },
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "yearmonth",
      "field": "Year",
      "title": "Year",
      "axis": {"format": "%Y"}
    },
    "y": {"aggregate": "sum", "field": "CO2_Emissions_Avoided_GtCO2"},
    "color": {"field": "Country", "scale": {"scheme": "tableau10"}},
    "tooltip":[{"field":"Country"},{"field":"CO2 Emissions Avoided (GtCO2)","format":".2f"}]
  },
  "config": {}
}
